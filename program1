#include <stdio.h>

int main() {
    int number_1, number_2, sum;
    printf("Enter the first number: ");
    scanf("%d", &number_1);
    printf("Enter the second number: ");
    scanf("%d", &number_2);
    sum = number_1 + number_2;
    printf("The sum of %d and %d is %d\n", number_1, number_2, sum);
    return 0;
}
